<h1>New Admin</h1>

<%= form_for(@admin, url: admins_path, method: :post) do |form| %>
  <div class="actions">
    <%= form.text_field :provinsi %>
  </div>

  <div class="actions">
    <%= form.text_field :jumlah %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


<%= link_to 'Back', admins_path %>
